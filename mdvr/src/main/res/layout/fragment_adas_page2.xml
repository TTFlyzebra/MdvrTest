                    android:layout_height="match_parent"
                    android:layout_toStartOf="@+id/adas_set_iv01_right"
                    android:layout_toEndOf="@+id/adas_set_iv01_left"
                    android:progressDrawable="@drawable/media_seekbar_style"
                    android:splitTrack="false"
                    android:thumb="@drawable/media_seekbar_thumb" />

                <ImageView
                    android:id="@+id/adas_set_iv01_right"
                    android:layout_width="30dp"
                    android:layout_height="wrap_content"
                    android:layout_toStartOf="@+id/adas_set_tv01"
                    android:src="@drawable/ic_keyboard_arrow_right" />

                <TextView
                    android:id="@+id/adas_set_tv01"
                    android:layout_width="100dp"
                    android:layout_height="match_parent"
                    android:layout_alignParentEnd="true"
                    android:gravity="end|center"
                    android:textColor="@color/white" />
            </RelativeLayout>
        </LinearLayout>

