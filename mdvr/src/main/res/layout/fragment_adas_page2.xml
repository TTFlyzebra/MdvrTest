<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/adas_page2"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="40dp"
        android:background="#7F000000"
        android:orientation="vertical">
        <!--horizon-->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginStart="12dp"
            android:background="@drawable/item_rectangle"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="4"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    android:text="@string/horizontal_line"
                    android:textColor="@color/white"
                    android:textSize="16sp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    android:text=" ----- "
                    android:textColor="#FF0000"
                    android:textSize="16sp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    android:text=")"
                    android:textColor="@color/white"
                    android:textSize="16sp" />
            </LinearLayout>

            <ImageButton
                android:id="@+id/adas_cali_horizont_up"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="#00000000"
                android:src="@drawable/ic_keyboard_arrow_up" />

            <TextView
                android:id="@+id/adas_cali_horizon_text"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:layout_weight="2"
                android:background="@drawable/edittext_bk"
                android:gravity="center"
                android:text="200"
                android:textColor="@color/white" />

            <ImageButton
                android:id="@+id/adas_cali_horizont_down"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="#00000000"
                android:src="@drawable/ic_keyboard_arrow_down" />
        </LinearLayout>

        <!--carMiddle-->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginStart="12dp"
            android:background="@drawable/item_rectangle"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="4"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    android:text="@string/car_central_line"
                    android:textColor="@color/white"
                    android:textSize="16sp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    android:text="  |  "
                    android:textColor="#FFFF00"
                    android:textSize="20sp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    android:text=")"
                    android:textColor="@color/white"
                    android:textSize="16sp" />
            </LinearLayout>

            <ImageButton
                android:id="@+id/adas_cali_carMiddle_left"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="#00000000"
                android:src="@drawable/ic_keyboard_arrow_left" />

            <TextView
                android:id="@+id/adas_cali_carMiddle_text"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:layout_weight="2"
                android:background="@drawable/edittext_bk"
                android:gravity="center"
                android:text="200"
                android:textColor="@color/white" />

            <ImageButton
                android:id="@+id/adas_cali_carMiddle_right"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="#00000000"
                android:src="@drawable/ic_keyboard_arrow_right" />
        </LinearLayout>

        <!--cameraHeight-->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginStart="12dp"
            android:background="@drawable/item_rectangle"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="4"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    android:text="@string/camera_height"
                    android:textColor="@color/white"
                    android:textSize="16sp" />
            </LinearLayout>

            <ImageButton
                android:id="@+id/adas_cali_cameraHeight_left"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="#00000000"
                android:src="@drawable/ic_keyboard_arrow_left" />

            <TextView
                android:id="@+id/adas_cali_cameraHeight_text"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:layout_weight="2"
                android:background="@drawable/edittext_bk"
                android:gravity="center"
                android:text="200"
                android:textColor="@color/white" />

            <ImageButton
                android:id="@+id/adas_cali_cameraHeight_right"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="#00000000"
                android:src="@drawable/ic_keyboard_arrow_right" />
        </LinearLayout>

        <!--cameraToAxle-->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginStart="12dp"
            android:background="@drawable/item_rectangle"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="4"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    android:text="@string/camera_to_axle"
                    android:textColor="@color/white"
                    android:textSize="16sp" />
            </LinearLayout>

            <ImageButton
                android:id="@+id/adas_cali_cameraToAxle_left"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="#00000000"
                android:src="@drawable/ic_keyboard_arrow_left" />

            <TextView
                android:id="@+id/adas_cali_cameraToAxle_text"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:layout_weight="2"
                android:background="@drawable/edittext_bk"
                android:gravity="center"
                android:text="200"
                android:textColor="@color/white" />

            <ImageButton
                android:id="@+id/adas_cali_cameraToAxle_right"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="#00000000"
                android:src="@drawable/ic_keyboard_arrow_right" />
        </LinearLayout>

        <!--carWidth-->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginStart="12dp"
            android:background="@drawable/item_rectangle"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="4"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    android:text="@string/car_width"
                    android:textColor="@color/white"
                    android:textSize="16sp" />
            </LinearLayout>

            <ImageButton
                android:id="@+id/adas_cali_carWidth_left"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="#00000000"
                android:src="@drawable/ic_keyboard_arrow_left" />

            <TextView
                android:id="@+id/adas_cali_carWidth_text"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:layout_weight="2"
                android:background="@drawable/edittext_bk"
                android:gravity="center"
                android:text="200"
                android:textColor="@color/white" />

            <ImageButton
                android:id="@+id/adas_cali_carWidth_right"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="#00000000"
                android:src="@drawable/ic_keyboard_arrow_right" />
        </LinearLayout>

        <!--cameraToBumper-->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginStart="12dp"
            android:background="@drawable/item_rectangle"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="4"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    android:text="@string/camera_to_bumper"
                    android:textColor="@color/white"
                    android:textSize="16sp" />
            </LinearLayout>

            <ImageButton
                android:id="@+id/adas_cali_cameraToBumper_left"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="#00000000"
                android:src="@drawable/ic_keyboard_arrow_left" />

            <TextView
                android:id="@+id/adas_cali_cameraToBumper_text"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:layout_weight="2"
                android:background="@drawable/edittext_bk"
                android:gravity="center"
                android:text="200"
                android:textColor="@color/white" />

            <ImageButton
                android:id="@+id/adas_cali_cameraToBumper_right"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="#00000000"
                android:src="@drawable/ic_keyboard_arrow_right" />
        </LinearLayout>

        <!--cameraToLeftWheel-->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginStart="12dp"
            android:background="@drawable/item_rectangle"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="4"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    android:text="@string/camera_to_left_wheel"
                    android:textColor="@color/white"
                    android:textSize="16sp" />
            </LinearLayout>

            <ImageButton
                android:id="@+id/adas_cali_cameraToLeftWheel_left"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="#00000000"
                android:src="@drawable/ic_keyboard_arrow_left" />

            <TextView
                android:id="@+id/adas_cali_cameraToLeftWheel_text"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:layout_weight="2"
                android:background="@drawable/edittext_bk"
                android:gravity="center"
                android:text="200"
                android:textColor="@color/white" />

            <ImageButton
                android:id="@+id/adas_cali_cameraToLeftWheel_right"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="#00000000"
                android:src="@drawable/ic_keyboard_arrow_right" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="5dp"
        android:gravity="center"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/bt_adas_cancel"
            android:layout_width="60dp"
            android:layout_height="match_parent"
            android:background="@drawable/text_button"
            android:gravity="center"
            android:padding="2dp"
            android:text="取消"
            android:textColor="@color/white" />

        <TextView
            android:id="@+id/bt_front_page"
            android:layout_width="60dp"
            android:layout_height="match_parent"
            android:background="@drawable/text_button"
            android:gravity="center"
            android:padding="2dp"
            android:text="上一页"
            android:textColor="@color/white" />

        <TextView
            android:id="@+id/bt_adas_save"
            android:layout_width="60dp"
            android:layout_height="match_parent"
            android:background="@drawable/text_button"
            android:gravity="center"
            android:padding="2dp"
            android:text="保存"
            android:textColor="@color/white" />
    </LinearLayout>
</RelativeLayout>