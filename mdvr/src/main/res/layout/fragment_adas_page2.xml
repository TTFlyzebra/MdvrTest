                    android:layout_width="30dp"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:src="@drawable/ic_keyboard_arrow_left" />

                <SeekBar
                    android:id="@+id/adas_set_sk01"
